{
  "name": "geochat_prototype",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:odedindi/GeoChat.git",
  "author": "Oded Winberger <odedindi@gmail.com>",
  "scripts": {
    "build:client": "cd client && yarn build",
    "install:client": "cd client && yarn install",
    "install:server": "cd server && yarn install",
    "start:client": "cd client && ionic serve",
    "start:server": "cd server && yarn dev",
    "dev": "concurrently \"yarn start:server\" \"yarn start:client\"",
    "install:": "concurrently \"yarn install:server\" \"yarn install:client\"",
    "format": "concurrently \"cd client && yarn format\" \"cd server && yarn format\""
  },
  "license": "MIT",
  "dependencies": {
    "concurrently": "^6.3.0",
    "observable-hooks": "^4.1.2"
  }
}
