  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "yarn clean && yarn lint && yarn tsc",
    "dev:start": "yarn build && yarn start",
    "dev": "nodemon --watch src -t ts --exec yarn dev:start",
    "start": "node .",
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "check:types": "tsc --pretty --noEmit",
    "check:lint": "tslint -c tslint.json -p tsconfig.json",
    "check": "yarn check:format && yarn check:types && yarn check:lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "socket.io": "^4.3.1",
    "socketio-jwt": "^4.6.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.4",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
