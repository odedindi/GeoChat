{
	"name": "server",
	"version": "0.0.1",
	"main": "dist/index.js",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf dist/*",
		"lint": "tslint -c tslint.json -p tsconfig.json --fix",
		"tsc": "tsc",
		"build": "yarn clean && yarn lint && yarn tsc",
		"dev:start": "yarn build && yarn start",
		"dev": "nodemon --watch ./src -e ts --exec yarn dev:start",
		"start": "node .",
		"format": "prettier --write .",
		"check:format": "prettier --check .",
		"check:types": "tsc --pretty --noEmit",
		"check:lint": "tslint -c tslint.json -p tsconfig.json",
		"check": "yarn check:format && yarn check:types && yarn check:lint",
		"test": "jest"
	},
	"dependencies": {
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"socket.io": "^4.3.1",
		"ts-node": "^10.4.0",
		"uuid": "^8.3.2",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@types/compression": "^1.7.2",
		"@types/express": "^4.17.13",
		"@types/jest": "^27.0.2",
		"@types/lodash": "^4.14.176",
		"@types/node": "^16.11.4",
		"@types/supertest": "^2.0.11",
		"@types/uuid": "^8.3.1",
		"jest": "^27.3.1",
		"nodemon": "^2.0.14",
		"prettier": "^2.4.1",
		"rimraf": "^3.0.2",
		"socket.io-client": "^4.3.2",
		"supertest": "^6.1.6",
		"ts-jest": "^27.0.7",
		"tslint": "^6.1.3",
		"typescript": "^4.4.4"
	}
}
